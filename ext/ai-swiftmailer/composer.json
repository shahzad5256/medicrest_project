{
	"name": "aimeos/ai-swiftmailer",
	"description": "Aimeos SwiftMailer adapter",
	"keywords": ["aimeos", "adapter", "swiftmailer", "mail"],
	"homepage": "https://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-swiftmailer",
		"issues": "https://github.com/aimeos/ai-swiftmailer/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aimeos/php-coveralls.git"
		}
	],
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"swiftmailer/swiftmailer": ">=4.0"
	},
	"require-dev": {
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
